def executeBuild() {

	try{
	        bat 'dir';
		println env.WORKSPACE;
		println env.FIRSTPROPERTY;
		println env.FIRSTPROPERTY1;
                
		
		}
		catch(Exception e){
				 return;			
			}
			
}
			
		







